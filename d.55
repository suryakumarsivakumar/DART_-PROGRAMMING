void main() {
  
  List<Student> students=[Student('surya'),
                         Student('karthi'),
                         Student('muruga'),
                         Student('shiva')];
  
  
  
  
  
  students.add(Student('New kid'));
  
  print(students);
  
  final student = students[4];
  print(student.name);
  
} 
  
  class Student{
    final String name;
    
    Student(this.name);
    
    

    @override
    String toString() => 'Student:$name';
  }


