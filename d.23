class Person{
  String firstname;
  String lastname;
  int age;
  bool isemployed;
  
  
  Person(String fname,String lname,int age,bool isemployed)
    :this.firstname=fname,
    this.lastname=lname,
    this.age=age,
    this.isemployed=isemployed;
  
}

void main() {
  Person person = Person("surya","kumar",21,true);
  print(person.firstname);
  print(person.lastname);
  print(person.age);
  print(person.isemployed);
  
    
 
}


